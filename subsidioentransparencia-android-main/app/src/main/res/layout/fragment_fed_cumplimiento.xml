                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="5dp"
                        android:font="@font/montserrat_regular_family"
                        android:textAlignment="center"
                        android:textColor="@color/gob_pink"
                        android:textSize="15sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/textCumplimientoSEPFed"
                        app:layout_constraintTop_toBottomOf="@+id/textFechaCumplimientoSEPFed"
                        app:layout_constraintVertical_bias="0.217"
                        tools:text="00/00/0000" />

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginHorizontal="0.5dp"
                    android:layout_marginTop="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/contentCompromiso">


                    <androidx.cardview.widget.CardView
                        android:id="@+id/cardCumplioSEPFed"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_margin="10dp"
                        app:cardElevation="0dp"
                        app:cardBackgroundColor="@color/green"
                        app:cardCornerRadius="60dp"
                        app:layout_constraintBottom_toBottomOf="parent"
